{
  "name": "modules-talk",
  "description": "This is the repo for the talk I will give/gave for the dsmJS user group during the November 2020 meeting.",
  "license": "MIT",
  "private": true,
  "repository": "trevtrich/modules-talk",
  "bugs": "https://github.com/trevtrich/modules-talk/issues",
  "homepage": "https://github.com/trevtrich/modules-talk#readme",
  "author": "Trevor Richardson <npm@trevorrichardson.me> (trevorrichardson.me)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "clean": "rimraf ./lib",
    "start": "node ./lib/server",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:server": "webpack --env production --config webpack.config.server.babel.js",
    "pretest:integration": "run-s build",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:unit:base": "DEBUG=any jest --testPathPattern=src/.*\\.test\\.js$",
    "lint:peer": "npm ls >/dev/null"
  },
  "dependencies": {
    "@hapi/glue": "8.0.0",
    "@hapi/good": "8.2.4",
    "bunyan": "1.8.14",
    "dotenv-safe": "8.2.0",
    "good-bunyan": "3.0.1",
    "hapi-graceful-shutdown-plugin": "2.0.8"
  },
  "devDependencies": {
    "@babel/register": "7.11.5",
    "@form8ion/babel-preset": "1.6.32",
    "@form8ion/commitlint-config": "1.0.14",
    "@form8ion/eslint-config": "1.6.32",
    "@form8ion/eslint-config-jest": "1.0.13",
    "@form8ion/remark-lint-preset": "1.0.10",
    "@istanbuljs/nyc-config-babel": "3.0.0",
    "@travi/any": "2.0.13",
    "@travi/travis-lint": "1.0.1",
    "babel-plugin-istanbul": "6.0.0",
    "ban-sensitive-files": "1.9.12",
    "chai": "4.2.0",
    "check-engine": "1.8.1",
    "codecov": "3.8.0",
    "cucumber": "7.0.0-rc.0",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.1.3",
    "http-status-codes": "2.1.4",
    "husky": "4.3.0",
    "jest": "26.5.2",
    "jest-when": "2.7.2",
    "lockfile-lint": "4.3.7",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "8.0.1",
    "remark-toc": "7.0.0",
    "rimraf": "3.0.2",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  }
}
